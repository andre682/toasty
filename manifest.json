{
    "manifest_version": 2,
    "name": "Toasty_LC",
    "version": "1.0",
    "description": "Toastyyy",
    "permissions": [
        "notifications"
    ],
    "web_accessible_resources": [
        "static/toasty.png",
        "static/toasty.mp3",
        "toasty.css"
    ],
    "content_scripts": [
        {
            "css": [
                "toasty.css"
            ],
            "js": [
                "jquery.min.js",
                "toasty.js"
            ],
            "matches": [
                "https://atlassian.net/wiki/spaces/NXCF/*"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "images/icon.png"
    },
    "icons": {
        "256": "images/icon.png"
    }
}
